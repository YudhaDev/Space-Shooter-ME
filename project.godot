config_version=5

[application]

config/name="Space Shooter ME"
run/main_scene="res://scenes/ui/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

DebugVars="*res://scripts/debug/GlobalDebugVars.gd"
BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
GlobalCurrencies="*res://scripts/global/GlobalCurrencies.gd"
GlobalEnvironment="*res://scripts/global/GlobalEnvironment.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=854
window/stretch/mode="viewport"
window/dpi/allow_hidpi=false
window/handheld/orientation=1

[dotnet]

project/assembly_name="Space Shooter ME"

[editor_plugins]

enabled=PackedStringArray("res://addons/better-terrain/plugin.cfg")

[gui]

common/snap_controls_to_pixels=false

[importer_defaults]

